m,n = int(input()), int(input())
mylist = [x for x in range(1,m+1)]
mylist2 = [y for y in range(1,n+1)]
counter = 0
for i in mylist:
  for j in mylist2:
    if i+j == 10:
      counter += 1

if counter == 1:
  print("There is 1 way to get the sum 10.")
else:
  print(f"There are {counter} ways to get the sum 10.")
